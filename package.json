{
  "name": "taishi-web-workflow",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "pentagon": "run-p type-check build-only-pt",
    "build-app": "npm run pentagon",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only-pt": "vite build --mode=pentagon",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "precommit": "npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logicflow/core": "^1.1.31",
    "@logicflow/extension": "^1.1.31",
    "ant-design-vue": "^3.2.15",
    "moment": "^2.29.4",
    "pinia": "^2.0.26",
    "rb-vue-plugins": "^1.0.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rb-vite-plugins": "^2.0.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vite-plugin-dynamic-base": "^0.4.7",
    "vite-plugin-externals": "^0.5.1",
    "vue-tsc": "^1.0.9"
  }
}